<launch>
  
  <arg name="model" default="$(env TURTLEBOT3_MODEL)" doc="model type [burger, waffle, waffle_pi]"/>
  <arg name="x_pos" default="0.0"/>   <!-- default="-3.0"/> -->
  <arg name="y_pos" default="0.0"/>   <!-- default=" 1.0"/> -->
  <arg name="z_pos" default="0.0"/>   <!-- default=" 0.0"/> -->

  <!-- -->
  <include file="$(find actor_sim_utils)/launch/example_full.launch">
    <arg name="world" value="parking"/>
  </include>

  <!--
  <param name="robot_description" command="$(find xacro)/xacro - -inorder $(find turtlebot3_description)/urdf/turtlebot3_$(arg model).urdf.xacro" />
  <node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" args="-urdf -model turtlebot3 -x $(arg x_pos) -y $(arg y_pos) -z $(arg z_pos) -param robot_description" />
  -->

  <param name="robot_description" command="$(find xacro)/xacro --inorder $(find turtlebot3_description)/urdf/turtlebot3_$(arg model).urdf.xacro" />
  <node pkg="gazebo_ros" type="spawn_model" name="spawn_urdf" args="-urdf -model turtlebot3_$(arg model) -x $(arg x_pos) -y $(arg y_pos) -z $(arg z_pos) -param robot_description" />

</launch>
